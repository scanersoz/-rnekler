# -*- coding:utf-8 -*-

import smtplib

fromadd = '******@gmail.com'
toadd = '******@gmail.com'

msg = '''hi,how r u'''
username = '*******@gmail.com'
passwd = '*******'

try:
    server = smtplib.SMTP('smtp.gmail.com:587')
    server.ehlo()
    server.starttls()
    server.login(username, passwd)

    server.sendmail(fromadd, toadd, msg)
        print("Mail Send Successfully")
        server.quit()

except:
    print("Error:unable to send mail")

    server.sendmail()
